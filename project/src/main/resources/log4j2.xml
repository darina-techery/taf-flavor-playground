<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
    <Appenders>
        <Console name="Clickable">
            <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level (%F:%L): %msg%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Root level="DEBUG">
            <AppenderRef ref="Clickable"/>
        </Root>
    </Loggers>
</Configuration>